<launch>
  <arg name="config_path" default="$(find-pkg-share multi_purpose_mpc_ros)/config/config.yaml"/>

  <node pkg="multi_purpose_mpc_ros" exec="run_mpc_controller.bash" name="mpc_controller" args="--config_path $(var config_path)" output="screen" >
    <param name="use_sim_time" value="true"/>
  </node>

</launch>
